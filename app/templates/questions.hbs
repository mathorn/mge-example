
{{#each model as |question|}}
  {{question-item question}}
{{else}}
  {{#if (or model.isLoaded error)}}
    <div class="alert alert-danger" role="alert">
      Keine Einträge gefunden
    </div>
  {{else}}
    <div class="alert alert-warning" role="alert">
      Lade Einträge...
    </div>
  {{/if}}
{{/each}}